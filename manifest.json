{

  "manifest_version": 2,
  "name": "TLDR",
  "version": "1.0",

  "description": "Provides info about terms of use.",
  "permissions": [
    "activeTab",
    "tabs",
    "https://tldr-sails.herokuapp.com/",
    "https://stackpath.bootstrapcdn.com/"
  ],

  "browser_action": {
    "browser_style": true,
    "default_title": "TLDR",
    "default_popup": "./popup/tldr.html"

  },
  "background": {
    "scripts": [
      "main.js"
    ]
  }

}
